{
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        },
        "files": ["lib/parser.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.4",
        "doctrine/lexer": "^2.0",
        "nikic/php-parser": "^4.15"
    },
    "require-dev": {
        "phpbench/phpbench": "^1.2",
        "maddhatter/markdown-table": "^1.1"
    },
    "scripts": {
        "bench-table": "php vendor/bin/phpbench run --report=my-report tests"
    }
}
